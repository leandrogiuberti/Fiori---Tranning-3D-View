{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sap.fe.core.fpmExplorer.draftValidation",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [""],
			"fallbackLocale": ""
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/fe/core/mock/draftValidation/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"odataVersion": "4.0"
				}
			}
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.111.1",
			"libs": {
				"sap.ui.core": {},
				"sap.fe.templates": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"": {
				"dataSource": "mainService",
				"settings": {
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			}
		},
		"routing": {
			"routes": [
				{
					"pattern": "/RootEntity({key}):?query:",
					"name": "default",
					"target": "sample"
				}
			],
			"targets": {
				"sample": {
					"type": "Component",
					"id": "SampleObjectPage",
					"name": "sap.fe.templates.ObjectPage",
					"viewLevel": 1,
					"options": {
						"settings": {
							"contextPath": "/RootEntity",
							"editableHeaderContent": false,
							"sectionLayout": "Page",
							"content": {
								"header": {
									"anchorBarVisible": false
								},
								"body": {
									"sections": {
										"Description": {
											"template": "sap.fe.core.fpmExplorer.Description",
											"title": "Description"
										},
										"SimpleField": {
											"template": "sap.fe.core.fpmExplorer.SimpleField",
											"title": "A field configured with the current context"
										},
										"SimpleTable": {
											"template": "sap.fe.core.fpmExplorer.SimpleTable",
											"title": "A table displaying a child entity set without global SideEffects"
										},
										"SimpleTableWithSE": {
											"template": "sap.fe.core.fpmExplorer.SimpleTableWithSE",
											"title": "A table displaying a child entity set with global SideEffects"
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}
}
