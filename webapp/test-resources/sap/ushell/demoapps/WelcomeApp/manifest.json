{
    "_version": "1.4.0",

    "sap.fiori": {
        "registrationIds": ["F9999999999999"]
    },

    "sap.app": {
        "_version": "1.1.0",
        "id": "sap.ushell.demoapps.WelcomeApp",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "title": "Home of S4",
        "subTitle": "{{subtitle}}",
        "description": "{{description}}",
        "applicationVersion": {
            "version": "1.1.0"
        },
        "ach": "CA-UI2-INT-FE",
        "dataSources": {},
        "cdsViews": [],
        "offline": true
    },
    "sap.ui": {
        "_version": "1.1.0",

        "technology": "UI5",
        "icons": {
             "icon": "sap-icon://Fiori2/F0003"
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        },
        "fullWidth": false
    },
    "sap.ui5": {
        "_version": "1.1.0",
        "resources": {
            "js": [],
            "css": [{
                "uri": "css/custom.css",
                "id": "sap.ushell.demoapps.WelcomeApp.stylesheet"
            }]
        },
        "dependencies": {
            "minUI5Version":"1.28",
            "libs": {
                "sap.m": {
                    "minVersion": "1.28"
                }
            }
        },
        "models": {},
        "rootView": {
            "viewName": "sap.ushell.demoapps.WelcomeApp.view.Main",
            "type": "XML",
			"async": true,
			"id": "app"
        },
        "handleValidation": false,
        "config": {
            "fullWidth": true
        },
        "routing": {},
        "contentDensities": {
            "compact": false,
            "cozy": true
        },
        "services": {
            "ShellUIService": {
                "factoryName": "sap.ushell.ui5service.ShellUIService"
            },
            "CrossApplicationNavigation": {
                "factoryName": "sap.ushell.ui5service.CrossApplicationNavigation"
            },
            "URLParsing": {
                "factoryName": "sap.ushell.ui5service.URLParsing"
            }
        }
    }
}
