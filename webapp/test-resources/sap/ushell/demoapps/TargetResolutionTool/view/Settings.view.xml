<mvc:View
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:layout="sap.ui.layout"
    controllerName="sap.ushell.demo.TargetResolutionTool.view.Settings">
    <layout:VerticalLayout class="sapUiContentPadding" width="100%">
        <layout:content>
            <Label text="Inbounds used during resolution" />
            <Button
                id="btnLoadCurrentConfiguration"
                enabled="true"
                text="Load current configuration"
                press="onBtnLoadCurrentInboundsPress" />
            <TextArea id="textIntent" rows="20" width="100%" value="{/inboundConfiguration}" editable="true" />
        </layout:content>
    </layout:VerticalLayout>
</mvc:View>
