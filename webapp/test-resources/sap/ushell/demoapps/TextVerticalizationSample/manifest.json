{
    "_version": "1.0.0",
    "sap.fiori": {
      "registrationIds": [""]
    },
    "sap.app": {
      "id": "sap.ushell.demo.textverticalization",
      "_version": "1.0.0",
      "i18n": {
        "bundleUrl": "i18n/i18n.properties",
        "terminologies": {
          "sports": {
              "bundleUrl": "i18n/i18n.sports.properties"
          },
          "travel": {
            "bundleUrl": "i18n/i18n.travel.properties"
          }
        }
      },
      "type": "application",
      "applicationVersion": {
        "version": "1.0.0"
      },
      "title": "{{title}}",
      "description": "{{description}}",
      "tags": {
        "keywords": [
          "{{keyword.sample}}",
          "{{keyword.demo}}",
          "{{keyword.flp}}"
        ]
      },
      "ach": "CA-FLP-FE-UI",
      "crossNavigation": {
        "inbounds": {
          "inb": {
            "semanticObject": "Action",
            "action": "toVerticalization",
            "signature": {
              "parameters": {},
              "additionalParameters": "allowed"
            }
          }
        }
      }
    },
    "sap.ui": {
      "_version": "1.0.0",
      "technology": "UI5",
      "icons": {
        "icon": "sap-icon://translate"
      },
      "deviceTypes": {
        "desktop": true,
        "tablet": true,
        "phone": true
      }
    },
    "sap.ui5": {
      "_version": "1.1.0",
      "dependencies": {
        "minUI5Version": "1.28",
        "libs": {
          "sap.m": {
            "minVersion": "1.28"
          }
        }
      },
      "models": {
        "i18n": {
          "type": "sap.ui.model.resource.ResourceModel",
          "settings": {
            "bundleName": "sap.ushell.demo.textverticalization.i18n.i18n",
            "async": true,
            "terminologies": {
              "sports": {
                "bundleUrl": "i18n/i18n.sports.properties"
              },
              "travel": {
                "bundleUrl": "i18n/i18n.travel.properties"
              }
            }
          }
        }
      },
      "handleValidation": false,
      "contentDensities": {
        "compact": false,
        "cozy": true
      }
    }
  }
