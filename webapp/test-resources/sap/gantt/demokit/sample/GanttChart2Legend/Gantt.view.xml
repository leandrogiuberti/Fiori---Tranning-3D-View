<mvc:View
		controllerName="sap.gantt.sample.GanttChart2Legend.Gantt"
		height="100%"
		xmlns="sap.gantt.simple"
		xmlns:axistime="sap.gantt.axistime"
		xmlns:config="sap.gantt.config"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:shapes="sap.gantt.simple.shapes"
		xmlns:table="sap.ui.table"
		xmlns:m="sap.m"
		xmlns:rm="sap.ui.table.rowmodes">
	<GanttChartContainer>
		<toolbar>
			<ContainerToolbar showLegendButton="true" showSearchButton="false">
				<legendContainer>
					<LegendContainer height="400px" width="300px">
						<ListLegend title="List Legend">
							<ListLegendItem legendName="Base Conditional Shape With LegendGroup">
							<legendShapeGroup>
								<LegendShapeGroup title="Base Conditional Shape With LegendGroup">
									<shapes>
										<BaseConditionalShape activeShape="{/baseConditionalShapeGroupType}">
											<shapes>
												<BaseTriangle fill="@sapUiChartPaletteQualitativeHue6"/>
												<BaseRectangle fill="@sapUiChartPaletteQualitativeHue6"/>
												<BaseDiamond fill="@sapUiChartPaletteQualitativeHue6"/>
												<LegendShapeGroup orientation="Horizontal" title="Single Shape">
													<shapes>
														<BaseDiamond fill="#FF0000" height="16" width="8" stroke="sapNeutralTextColor" tooltip="ABCDS"/>
														<BaseRectangle fill="@sapUiChartPaletteQualitativeHue6" height="16" width="8"/>
													</shapes>
												</LegendShapeGroup>
												<LegendShapeGroup orientation="Vertical" title="Single Shape">
													<shapes>
														<BaseRectangle fill="#0000FF" height="16" width="16"/>
													</shapes>
												</LegendShapeGroup>
											</shapes>
										</BaseConditionalShape>
									</shapes>
								</LegendShapeGroup>
                    		</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project1">
								<shapes:Task title="Task in project 1" color="sapUiLegend18" type="Normal"/>
							</ListLegendItem>
							<ListLegendItem legendName="project2">
								<shapes:Task title="Project 2" color="sapUiLegend15" type="SummaryExpanded"/>
							</ListLegendItem>
							<ListLegendItem legendName="project2">
								<shapes:Task title="Task in project 2" color="sapUiLegend15" type="Normal"/>
							</ListLegendItem>
							<ListLegendItem legendName="project2">
								<shapes:Task title="Error task" color="sapUiLegend15" type="Error"/>
							</ListLegendItem>
							<ListLegendItem legendName="project2">
								<legendShapeGroup>
									<LegendShapeGroup orientation="Horizontal" title="Horizontal Diamond">
										<shapes>
											<BaseRectangle fill="#BAC1C4" height="16" width="4"/>
											<BaseDiamond fill="sapNeutralTextColor" height="16" width="8" stroke="sapNeutralTextColor" tooltip="ABCDS"/>
											<BaseRectangle fill="#0000FF" height="16" width="4"/>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Vertical" title="Vertical Diamond">
										<shapes>
											<BaseDiamond fill="sapNeutralTextColor" height="8" width="16" stroke="sapNeutralTextColor" tooltip="ABCDS"/>
											<BaseRectangle fill="#BAC1C4" height="4" width="16"/>
											<BaseRectangle fill="#FF0000" height="4" width="16"/>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Vertical" title="Single Shape">
										<shapes>
											<BaseRectangle fill="#BAC1C4" height="16" width="16"/>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Overlap" title="Overlap Rectangle">
										<shapes>
											<BaseRectangle fill="#BAC1C4" height="16" width="16"/>
											<BaseRectangle fill="#FF0000" height="12" width="12"/>
											<BaseRectangle fill="sapUiLegend18" height="8" width="8" />
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2">
								<legendShapeGroup>
									<LegendShapeGroup orientation="Horizontal" title="Horizontal Chevron">
										<shapes>
											<BaseRectangle fill="#BAC1C4" height="16" width="4"/>
											<BaseChevron fill="sapNeutralTextColor" height="16" width="8" stroke="sapNeutralTextColor" tooltip="ABCDS"/>
											<BaseRectangle fill="#0000FF" height="16" width="4"/>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Vertical" title="Vertical Chevron">
										<shapes>
											<BaseChevron fill="sapNeutralTextColor" height="8" width="16" stroke="sapNeutralTextColor" tooltip="ABCDS"/>
											<BaseRectangle fill="#BAC1C4" height="4" width="16"/>
											<BaseRectangle fill="#FF0000" height="4" width="16"/>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Horizontal" title="Horizontal Cursor">
										<shapes>
											<BaseRectangle fill="#BAC1C4" height="16" width="4"/>
											<BaseCursor fill="sapNeutralTextColor" length="8" width="12" pointHeight="4" stroke="sapNeutralTextColor" tooltip="ABCDS"/>
											<BaseRectangle fill="#FF0000" height="16" width="4"/>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Vertical" title="Vertical Cursor">
										<shapes>
											<BaseCursor fill="sapNeutralTextColor" length="10" width="5" pointHeight="5" stroke="sapNeutralTextColor" tooltip="ABCDS"/>
											<BaseRectangle fill="#BAC1C4" height="4" width="16"/>
											<BaseRectangle fill="#FF0000" height="4" width="16"/>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Horizontal" title="Horizontal Line">
										<shapes>
											<BaseRectangle fill="#BAC1C4" height="16" width="8"/>
											<BaseLine x1="9" x2="9" y1="0" y2="16" stroke="sapNeutralTextColor" strokeWidth="2" tooltip="ABCDS"/>
											<BaseRectangle fill="#FF0000" height="16" width="8"/>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Vertical" title="Vertical Line">
										<shapes>
											<BaseRectangle fill="#BAC1C4" height="4" width="16"/>
											<BaseLine x1="0" x2="16" y1="5" y2="5" stroke="sapNeutralTextColor" strokeWidth="2" tooltip="ABCDS"/>
											<BaseRectangle fill="#FF0000" height="4" width="16"/>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Horizontal" title="Horizontal Image">
										<shapes>
											<BaseImage height="16" width="8" src="test-resources/sap/gantt/demokit/sample/GanttChart2Legend/img.png" />
											<BaseRectangle fill="#BAC1C4" height="16" width="4"/>
											<BaseRectangle fill="#FF0000" height="16" width="4"/>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Vertical" title="Vertical Image">
										<shapes>
											<BaseImage height="8" width="16" src="test-resources/sap/gantt/demokit/sample/GanttChart2Legend/img.png" />
											<BaseRectangle fill="#BAC1C4" height="4" width="16"/>
											<BaseRectangle fill="#FF0000" height="4" width="16"/>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Vertical" title="Path">
										<shapes>
											<BasePath d="M12 0 l3 5.25-3 5.25h-6l-3-5.25 3-5.25z" fill="#FF0000"/>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Vertical" title="Multiple Group 1">
										<shapes>
											<BaseRectangle fill="sapPositiveElementColor" height="8" width="16"/>
											<BaseRectangle fill="sapButton_Attention_BorderColor" height="8" width="16"/>
											<LegendShapeGroup orientation="Horizontal">
												<shapes>
													<BaseRectangle fill="#FF0000" height="16" width="8"/>
												</shapes>
											</LegendShapeGroup>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Horizontal" title="Multiple Group 2">
										<shapes>
											<BaseRectangle fill="sapPositiveElementColor" height="16" width="8"/>
											<BaseRectangle fill="sapButton_Attention_BorderColor" height="16" width="8"/>
											<LegendShapeGroup orientation="Vertical">
												<shapes>
													<BaseRectangle fill="#FF0000" height="8" width="16"/>
												</shapes>
											</LegendShapeGroup>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="project2" >
								<legendShapeGroup>
									<LegendShapeGroup orientation="Vertical" title="Multiple Group 3">
										<shapes>
											<BaseRectangle fill="sapPositiveElementColor" height="8" width="16"/>
											<BaseRectangle fill="sapButton_Attention_BorderColor" height="8" width="16"/>
											<LegendShapeGroup orientation="Vertical">
												<shapes>
													<BaseRectangle fill="#FF0000" height="8" width="8"/>
													<BaseRectangle fill="#0000FF" height="8" width="8"/>
												</shapes>
											</LegendShapeGroup>
										</shapes>
									</LegendShapeGroup>
								</legendShapeGroup>
							</ListLegendItem>
							<ListLegendItem legendName="Base Conditional Shape">
								<shape>
									<BaseConditionalShape activeShape="{/baseConditionalShapeType}" title="Base Conditional Shape">
										<shapes>
											<BaseTriangle fill="@sapUiChartPaletteQualitativeHue6"/>
											<BaseRectangle fill="@sapUiChartPaletteQualitativeHue6"/>
											<BaseDiamond fill="@sapUiChartPaletteQualitativeHue6"/>
										</shapes>
									</BaseConditionalShape>
								</shape>
							</ListLegendItem>
						</ListLegend>
					</LegendContainer>
				</legendContainer>
			</ContainerToolbar>
		</toolbar>
		<GanttChartWithTable id="gantt" shapeSelectionMode="Single">
			<axisTimeStrategy>
				<axistime:ProportionZoomStrategy>
					<axistime:totalHorizon>
						<config:TimeHorizon
								startTime="20171001000000"
								endTime="20191129000000"/>
					</axistime:totalHorizon>
					<axistime:visibleHorizon>
						<config:TimeHorizon
								startTime="20181001000000"
								endTime="20181129000000"/>
					</axistime:visibleHorizon>
				</axistime:ProportionZoomStrategy>
			</axisTimeStrategy>
			<table>
				<table:TreeTable
						selectionMode="Single"
						selectionBehavior="RowSelector"
						enableColumnReordering="true"
						rows="{
						path: '/root',
						parameters: {
							arrayNames: ['children'],
							numberOfExpandedLevels: 1
						}
					}">
					<table:columns>
						<table:Column>
							<m:Text text=""/>
							<table:template>
								<m:Label text="{text}"/>
							</table:template>
						</table:Column>
					</table:columns>
					<table:rowMode>
						<rm:Auto minRowCount="12"/>
					</table:rowMode>
					<table:rowSettingsTemplate>
						<GanttRowSettings rowId="{id}" shapes1="{path: 'tasks', templateShareable:false}" shapes2="{path: 'legendShapes', templateShareable:false}">
							<shapes1>
								<shapes:Task time="{path: 'startTime', formatter: '.fnTimeConverter'}" selectable="true"
											 endTime="{path: 'endTime', formatter: '.fnTimeConverter'}"
											 type="{type}" color="{= ${projectId} === 1 ? 'sapUiLegend18' : 'sapUiLegend15'}" shapeId="{id}"/>
							</shapes1>
							<shapes2>
                            	<BaseConditionalShape activeShape="{ path:&apos;shape&apos; }" shapeId="{id}">
                            		<shapes>                          
										<BaseDiamond time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="sapNeutralTextColor"
											height="14" yBias="-10" width="75"/>
										<BaseRectangle time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="#BAC1C4"
											height="10" yBias="0" />        
										<BaseRectangle time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="sapNegativeElementColor"
											height="10" yBias="10" />
										<BaseRectangle time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="#BAC1C4"
											height="32" yBias="0" width="10"/>
										<BaseDiamond time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="sapNeutralTextColor"
											height="32" yBias="0" width="65"/>
										<BaseRectangle time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="sapInformativeElementColor"
											height="32" yBias="0" width="10"/>
										<BaseRectangle time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="#BAC1C4"
											height="32" yBias="0" width="35"/>
										<BaseCursor time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="sapNeutralTextColor"
											height="20" yBias="-8" width="15"/>
										<BaseRectangle time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="#BAC1C4"
											height="8" yBias="6" />     
										<BaseRectangle time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="sapNegativeElementColor"
											height="8" yBias="13" />                                
										<BaseImage time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="sapCriticalElementColor"
											height="20" yBias="0" width="35" src="test-resources/sap/gantt/demokit/sample/GanttChart2Legend/img.png"/>                                                                  
										<BaseRectangle time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="#BAC1C4"
											height="32" yBias="0" width="15"/>
										<BaseRectangle time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="sapNegativeElementColor"
											height="32" yBias="0" width="12"/>
										<BaseDiamond time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="#FF0000"
											height="32" yBias="0" width="75"/>
										<BaseRectangle time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="@sapUiChartPaletteQualitativeHue6"
											height="32" yBias="0" />
										<BaseTriangle time="{path: &apos;startTime&apos;, formatter: &apos;.fnTimeConverter&apos;}"
											endTime="{path: &apos;endTime&apos;, formatter: &apos;.fnTimeConverter&apos;}" fill="@sapUiChartPaletteQualitativeHue6"
											height="32" width="75" yBias="-16" />
                            		</shapes>
                            	</BaseConditionalShape>
                            </shapes2>                          
						</GanttRowSettings>
					</table:rowSettingsTemplate>
				</table:TreeTable>
			</table>
		</GanttChartWithTable>
	</GanttChartContainer>
</mvc:View>