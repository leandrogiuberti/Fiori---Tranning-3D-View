<mvc:View controllerName="accessingVisService.controller.App"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:vk="sap.ui.vk"
	displayBlock="false">
	<Shell>
		<App id="accessingVisService">
			<Page title="{i18n>pageTitle}">
				<HBox justifyContent="SpaceBetween" alignItems="Center" width="100%" height="100%">
					<VBox renderType="Bare" height="100%" width="100%">
						<HBox justifyContent="Center" alignItems="Center" width="100%">
							<Button text="Load Clutch Model" press=".onLoadClutch"/>
						</HBox>
						<vk:Viewport id="viewport" width="100%" height="100%"/>
					</VBox>
				</HBox>
			</Page>
		</App>
	</Shell>
</mvc:View>
