<mvc:View
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:smartForm="sap.ui.comp.smartform"
		xmlns:smartField="sap.ui.comp.smartfield"
		controllerName="applicationUnderTest.controller.SmartForm_sematicGroupElement"
		height="100%">
		<smartForm:SmartForm id="smartForm" editTogglable="true" title="Smart Form" expanded="true">
			<smartForm:Group title="Group1">
				<smartForm:SemanticGroupElement id="SemanticGroupElement1">
					<smartField:SmartField id="Office" value="{Office}" visible="true"/>
					<smartField:SmartField id="Office1" value="{Office}" visible="true"/>
					<smartField:SmartField id="Name" value="{Name}" visible="true" textInEditModeSource="ValueListNoValidation"/>
				</smartForm:SemanticGroupElement>
			</smartForm:Group>
			<smartForm:Group title="Group2">
				<smartForm:GroupElement>
					<smartField:SmartField id="Office2" value="{Office}" visible="true"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="Office22" value="{Office}" visible="true"/>
				</smartForm:GroupElement>
				<smartForm:GroupElement>
					<smartField:SmartField id="Name2" value="{Name}" visible="true" textInEditModeSource="ValueList"/>
				</smartForm:GroupElement>
			</smartForm:Group>
			<smartForm:Group title="Group3">
				<smartForm:SemanticGroupElement id="SemanticGroupElement2">
					<smartField:SmartField id="Office3" value="{Office}" visible="true"/>
					<smartField:SmartField id="Office33" value="{Office}" visible="true"/>
					<smartField:SmartField id="Name3" value="{Name}" visible="true" textInEditModeSource="ValueListNoValidation"/>
				</smartForm:SemanticGroupElement>
				<smartForm:GroupElement>
				</smartForm:GroupElement>
			</smartForm:Group>
			<smartForm:Group title="UoM and Currency">
				<smartForm:SemanticGroupElement id="SemanticGroupElementUoMCurrency">
					<smartField:SmartField id="Price" value="{Price}" visible="true"/>
					<smartField:SmartField id="Quantity" value="{Quantity}" visible="true"/>
				</smartForm:SemanticGroupElement>
				<smartForm:GroupElement>
				</smartForm:GroupElement>
			</smartForm:Group>
			<smartForm:Group title="UoM and Currency Not Editable">
				<smartForm:SemanticGroupElement id="SemanticGroupElementUoMCurrencyNotEditable">
					<smartField:SmartField id="PriceNotEditable" value="{PriceNotEditable}" visible="true"/>
					<smartField:SmartField id="QuantityNotEditable" value="{QuantityNotEditable}" visible="true"/>
				</smartForm:SemanticGroupElement>
			</smartForm:Group>
			<smartForm:Group title="SmartLinks and SmartFields with ObjectStatus">
				<smartForm:SemanticGroupElement>
					<smartField:SmartField id="category1" value="{Category}"/>
					<smartField:SmartField id="category2" value="{Category}"/>
				</smartForm:SemanticGroupElement>
				<smartForm:SemanticGroupElement>
					<smartField:SmartField id="office4" value="{Office}"/>
					<smartField:SmartField id="category3" value="{Category}"/>
					<smartField:SmartField id="status1" value="{Status}"/>
					<smartField:SmartField id="status2" value="{Status}"/>
				</smartForm:SemanticGroupElement>
			</smartForm:Group>
		</smartForm:SmartForm>
</mvc:View>
