<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0"
	xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
	xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
	xmlns:sap="http://www.sap.com/Protocols/SAPData">
	<edmx:DataServices m:DataServiceVersion="2.0">
		<Schema Namespace="EmployeesNamespace"
				xml:lang="en"
				sap:schema-version="1"
				xmlns="http://schemas.microsoft.com/ado/2008/09/edm">

			<EntityType Name="Employee"
						sap:label="Employees"
						sap:content-version="1">
				<Key>
					<PropertyRef Name="Id"/>
				</Key>
				<Property Name="Id"
							Type="Edm.String"
							Nullable="false"
							MaxLength="4"
							sap:visible="false"
							sap:label="Employee Id"
							sap:creatable="false"/>
				<Property Name="Name"
							Type="Edm.String"
							sap:creatable="false"
							sap:updatable="true"
							sap:label="Name"
							Nullable="false"/>
				<Property Name="Office"
							Type="Edm.String"
							sap:creatable="false"
							sap:updatable="true"
							sap:label="Office"
							Nullable="false"/>

				<Property Name="Visible"
							Type="Edm.Boolean"/>

				<Property Name="Price"
							Type="Edm.String"
							sap:unit="CurrencyCode"
							MaxLength="3"
							sap:label="Price" />

				<Property Name="CurrencyCode"
							Type="Edm.String"
							MaxLength="3"
							sap:label="Currency"
							sap:semantics="currency-code" />

				<Property Name="Quantity"
							Type="Edm.String"
							Nullable="true"
							sap:unit="UnitCode"
							MaxLength="3"
							sap:label="Quantity"
							sap:updatable="true"
							Precision="13"
							Scale="6"/>

				<Property Name="UnitCode"
							Type="Edm.String"
							Nullable="false"
							sap:semantics="unit-of-measure"
							MaxLength="3"
							sap:label="Unit"
							sap:updatable="true"/>

				<Property Name="PriceNotEditable"
							Type="Edm.String"
							sap:unit="CurrencyCodeNotEditable"
							MaxLength="3"
							sap:updatable="false"
							sap:label="Price Not Editable" />

				<Property Name="CurrencyCodeNotEditable"
							Type="Edm.String"
							MaxLength="3"
							sap:label="Currency Not Editable"
							sap:updatable="false"
							sap:semantics="currency-code" />

				<Property Name="QuantityNotEditable"
							Type="Edm.String"
							Nullable="true"
							sap:unit="UnitCodeNotEditable"
							MaxLength="3"
							sap:label="Quantity Not Editable"
							sap:updatable="false"
							Precision="13"
							Scale="6"/>

				<Property Name="UnitCodeNotEditable"
							Type="Edm.String"
							Nullable="false"
							sap:semantics="unit-of-measure"
							MaxLength="3"
							sap:label="Unit Not Editable"
							sap:updatable="false"/>

				<Property Name="Category"
							Type="Edm.String"
							sap:label="Category Link"/>

				<Property Name="Status"
							Type="Edm.String"
							sap:label="Status"/>

				<Property Name="StatusCriticality"
							Type="Edm.String"
							sap:label="Status Criticality"/>
			</EntityType>

			<EntityType Name="CompanyVHType" sap:content-version="1">
				<Key>
					<PropertyRef Name="KEY" />
				</Key>
				<Property Name="KEY" Type="Edm.String" sap:text="TXT" Nullable="false" MaxLength="20" sap:label="Key"/>
				<Property Name="Company" Type="Edm.String" sap:label="Company"/>
				<Property Name="TXT" Type="Edm.String" sap:label="Desc"/>
			</EntityType>

			<EntityType Name="CompanyDescriptionVHType" sap:content-version="1">
				<Key>
					<PropertyRef Name="KEY" />
				</Key>
				<Property Name="KEY" Type="Edm.String" sap:text="TXT" Nullable="false" MaxLength="20" sap:label="Company Key"/>
				<Property Name="TXT" Type="Edm.String" sap:label="Company Desc"/>
			</EntityType>

			<EntityContainer Name="EmployeesNamespace_Entities"
								m:IsDefaultEntityContainer="true"
								sap:supported-formats="atom json xlsx">

				<EntitySet Name="Employees"
							EntityType="EmployeesNamespace.Employee"
							sap:creatable="false"
							sap:updatable="true"
							sap:deletable="true"
							sap:content-version="1"/>

				<EntitySet Name="CompanyVH"
							EntityType="EmployeesNamespace.CompanyVHType"
							sap:creatable="false"
							sap:updatable="true"
							sap:deletable="false"
							sap:addressable="false"
							sap:content-version="1" />

				<EntitySet Name="CompanyDescriptionVH"
							EntityType="EmployeesNamespace.CompanyDescriptionVHType"
							sap:creatable="false"
							sap:updatable="true"
							sap:deletable="false"
							sap:addressable="false"
							sap:content-version="1" />

			</EntityContainer>

			<Annotations Target="EmployeesNamespace.Employee/Name"
							xmlns="http://docs.oasis-open.org/odata/ns/edm">

				<Annotation Term="com.sap.vocabularies.Common.v1.Text" Path="Name">
					<Annotation Term="com.sap.vocabularies.UI.v1.TextArrangement" EnumMember="com.sap.vocabularies.UI.v1.TextArrangementType/TextLast" />
				</Annotation>

				<Annotation Term="com.sap.vocabularies.Common.v1.ValueList">
					<Record>
						<PropertyValue Property="Label" String="Types"/>
						<PropertyValue Property="CollectionPath" String="CompanyVH"/>
						<PropertyValue Property="SearchSupported" Bool="true"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="com.sap.vocabularies.Common.v1.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="Name"/>
									<PropertyValue Property="ValueListProperty" String="KEY"/>
								</Record>
								<Record Type="com.sap.vocabularies.Common.v1.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="Company"/>
								</Record>
								<Record Type="com.sap.vocabularies.Common.v1.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="TXT"/>
								</Record>
								<Record Type="com.sap.vocabularies.Common.v1.ValueListParameterIn">
									<PropertyValue Property="LocalDataProperty" PropertyPath="Office"/>
									<PropertyValue Property="ValueListProperty" String="Company"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>

			<Annotations Target="EmployeesNamespace.Employee/Category" xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="com.sap.vocabularies.Common.v1.SemanticObject"
							String="smartlink_SemanticObjectName"/>
			</Annotations>

			<Annotations Target="EmployeesNamespace.Employee/Status" xmlns="http://docs.oasis-open.org/odata/ns/edm">
				<Annotation Term="com.sap.vocabularies.UI.v1.DataFieldDefault">
					<Record Type="com.sap.vocabularies.UI.v1.DataField">
						<PropertyValue Property="Criticality" Path="StatusCriticality"/>
						<PropertyValue Property="CriticalityRepresentation" EnumMember="com.sap.vocabularies.UI.v1.CriticalityRepresentationType/WithoutIcon"/>
					</Record>
				</Annotation>
			</Annotations>

			<Annotations Target="EmployeesNamespace.CompanyVHType/Company"
							xmlns="http://docs.oasis-open.org/odata/ns/edm">

				<Annotation Term="com.sap.vocabularies.Common.v1.Text" Path="TXT">
					<Annotation Term="com.sap.vocabularies.UI.v1.TextArrangement" EnumMember="com.sap.vocabularies.UI.v1.TextArrangementType/TextLast" />
				</Annotation>

				<Annotation Term="com.sap.vocabularies.Common.v1.ValueList">
					<Record>
						<PropertyValue Property="Label" String="Types"/>
						<PropertyValue Property="CollectionPath" String="CompanyDescriptionVH"/>
						<PropertyValue Property="SearchSupported" Bool="false"/>
						<PropertyValue Property="Parameters">
							<Collection>
								<Record Type="com.sap.vocabularies.Common.v1.ValueListParameterInOut">
									<PropertyValue Property="LocalDataProperty" PropertyPath="Company"/>
									<PropertyValue Property="ValueListProperty" String="KEY"/>
								</Record>
								<Record Type="com.sap.vocabularies.Common.v1.ValueListParameterDisplayOnly">
									<PropertyValue Property="ValueListProperty" String="TXT"/>
								</Record>
							</Collection>
						</PropertyValue>
					</Record>
				</Annotation>
			</Annotations>
		</Schema>
	</edmx:DataServices>
</edmx:Edmx>
