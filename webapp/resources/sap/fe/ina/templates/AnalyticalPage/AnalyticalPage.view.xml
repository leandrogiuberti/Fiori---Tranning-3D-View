<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:inaBlocks="sap.fe.ina.blocks"
	xmlns:layout="sap.ui.layout"
	xmlns:fl="sap.ui.fl.variants"
	xmlns:df_changeHandler="sap.sac.df.changeHandler"
	xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1"
	template:require="{
		ID: 'sap/fe/core/helpers/StableIdHelper',
		AP: 'sap/fe/ina/templates/AnalyticalPage/AnalyticalPageTemplating'
	}"
	controllerName="sap.fe.ina.templates.AnalyticalPage.AnalyticalPageController"
>
	<template:with path="converterContext>mainEntitySet" var="entitySet">
		<template:with path="converterContext>mainEntityType" var="entityType">
			<f:DynamicPage
				id="fe::AnalyticalPage"
				busy="{ui>/busy}"
				busyIndicatorDelay="0"
				class="{= !${converterContext>hasMultiVisualizations} &amp;&amp; ${converterContext>views}.length > 1 ? 'sapUiNoContentPadding' : 'sapUiResponsiveContentPadding'}"
			>
				<f:title>
					<f:DynamicPageTitle stateChange=".handlers.onDynamicPageTitleStateChanged">
						<f:heading>
							<fl:VariantManagement id="VariantManagement" for="fe::AnalyticalPage" />
						</f:heading>
						<f:snappedContent>
							<Text id="fe::appliedFiltersText" />
						</f:snappedContent>
					</f:DynamicPageTitle>
				</f:title>
				<f:header>
					<f:DynamicPageHeader pinnable="{converterContext>showPinnableToggle}">
						<inaBlocks:FilterBar id="{converterContext>filterBarId}" metaPath="{converterContext>mainEntitySet}" />
					</f:DynamicPageHeader>
				</f:header>
				<f:content>
					<f:SidePanel id="fe::SidePanel" sidePanelResizable="true" sidePanelMinWidth="50rem" sidePanelPosition="Left">
						<f:mainContent>
							<inaBlocks:Grid
								id="fe::Grid"
								metaPath="{converterContext>mainEntitySet}"
								annotationPath="{visualizationDefinition>annotationPath}"
							/>
						</f:mainContent>
						<f:items>
							<f:SidePanelItem id="NavigationSidePanelItem" icon="sap-icon://provision" text="Layout" key="navigationPanel">
								<inaBlocks:DesignerPanel
									id="fe::FilterPanelMultiDimDesignerPanel"
									metaPath="{converterContext>mainEntitySet}"
									height="50rem"
								/>
							</f:SidePanelItem>
							<f:SidePanelItem id="LayoutSidePanelItem" icon="sap-icon://add-filter" text="Filter" key="builderPanel">
								<inaBlocks:FilterPanel id="fe::FilterPanel" metaPath="{converterContext>mainEntitySet}" />
							</f:SidePanelItem>
						</f:items>
					</f:SidePanel>
				</f:content>
				<f:dependents>
					<df_changeHandler:MultiDimModelChangeHandler id="MultiDimModelChangeHandler" />
				</f:dependents>
			</f:DynamicPage>
		</template:with>
	</template:with>
</mvc:View>
