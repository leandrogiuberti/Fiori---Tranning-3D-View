<?xml version="1.0" encoding="UTF8"?>
<interaction-documentation library="sap.ui.comp" xmlns="http://schemas.sap.com/sapui5/extension/sap.ui.core.InteractionDocumentation/1">
	<control-interactions>
		<control name="sap.ui.comp.smarttable.SmartTable"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Otevře dialog <span class="uicontrol">Nastavení tabulky</span>, pokud je dostupný.</p>
			</description>
		</interaction>
		<interaction> <kbd>Ctrl + Shift + E</kbd> <description>
				<p>Otevře dialog tabulky <span class="uicontrol">Exportovat jako</span>, pokud je dostupný.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.comp.smartfilterbar.SmartFilterbar"/>
		<interaction> <kbd>Enter</kbd> <description>
				<p>Spustí hledání <span class="uicontrol">Panelu inteligentního filtrování</span>.</p>
			</description>
		</interaction>
		<interaction> <kbd>Tab</kbd> <description>
				<p>Přesune fokus na další interaktivní prvek v řetězci záložek.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + Tab</kbd> <description>
				<p>Přesune fokus na předchozí interaktivní prvek v řetězci záložek.</p>
			</description>
		</interaction>
		<interaction> <kbd>F6</kbd> <description>
				<p>Přesune fokus na první interaktivní prvek mimo kontrolu v další skupině UI (skup. F6).</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + F6</kbd> <description>
				<p>Přesune fokus na první interaktivní prvek mimo kontrolu v předchozí skupině UI (skup. F6).</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.comp.smartform.SmartForm"/>
		<interaction> <kbd>Tab</kbd> <description>
				<p>Přesune fokus na další interaktivní prvek v řetězci záložek.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + Tab</kbd> <description>
				<p>Přesune fokus na předchozí interaktivní prvek v řetězci záložek.</p>
			</description>
		</interaction>
		<interaction> <kbd>F6</kbd> <description>
				<p>Přesune fokus na první interaktivní prvek další skupiny UI (skup. F6) v řetězci záložek.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + F6</kbd> <description>
				<p>Přesune fokus na první interaktivní prvek předchozí skupiny UI (skup. F6) v řetězci záložek.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.comp.smartfield.SmartField"/>
		<interaction> <kbd>F4</kbd> <description>
				<p>Otevře/zavře rozbalovací seznam nebo sbalitelné oblasti <span class="uicontrol">dialogu nápovědy hodnot</span>.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.comp.valuehelpdialog.ValueHelpDialog"/>
		<interaction> <kbd>Esc</kbd> <description>
				<p>Zruší a zavře <span class="uicontrol">dialog nápovědy hodnot.</span></p>
			</description>
		</interaction>
		<interaction> <kbd>Tab</kbd> <description>
				<p>Přesune fokus na další interaktivní prvek v <span class="uicontrol">dialogu nápovědy hodnot</span>. Je-li zaměřen poslední prvek, přesune se fokus na první interaktivní prvek v <span class="uicontrol">dialogu nápovědy hodnot</span>.</p>
			</description>
		</interaction>
		<interaction> <kbd>Shift + Tab</kbd> <description>
				<p>Přesune fokus na předchozí interaktivní prvek v <span class="uicontrol">dialogu nápovědy hodnot</span>.</p>
			</description>
		</interaction>
	</control-interactions>
	<control-interactions>
		<control name="sap.ui.comp.SmartChart"/>
		<interaction> <kbd>Ctrl + ,</kbd> <description>
				<p>Otevře dialog <span class="uicontrol">Nastavení pohledu</span></p>
			</description>
		</interaction>
	</control-interactions>
</interaction-documentation>