<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
    <Dialog
        id="pageNotExistsDialog"
        title="{i18n>PageRuntime.CannotLoadPage.ErrorTitle}"
        class="sapUiResponsivePadding"
        state="Error">
        <content>
            <VBox class="sapUiSmallMargin">
                <Text text="{errorPage>/details}" />
                <Text class="sapUiSmallMarginTop" text="{errorPage>/technicalError}" />
                <Text class="sapUiSmallMarginTop" text="{errorPage>/pageAndSpaceId}" />
            </VBox>
        </content>
        <beginButton>
            <Button
                id="pageNotExistsDialogCopyButton"
                text="{i18n>PageRuntime.CannotLoadPage.CopyButton}"
                press="._copyToClipboard" />
        </beginButton>
        <endButton>
            <Button
                id="pageNotExistsDialogCancelButton"
                text="{i18n>PageRuntime.CannotLoadPage.CloseButton}"
                press="._onDialogClose"
                type="Emphasized" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>
