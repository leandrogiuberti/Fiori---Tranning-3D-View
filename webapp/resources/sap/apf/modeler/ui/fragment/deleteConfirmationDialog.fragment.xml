<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog xmlns="sap.m" id="idDeleteConfirmation" showHeader="true"
		class="sapUiSizeCompact dialogContentPadding" type="Standard" initialFocus="idDeleteButton">
		<beginButton>
			<Button xmlns="sap.m" id="idDeleteButton" press="handleConfirmDeletion" type="Emphasized">
			</Button>
		</beginButton>
		<endButton>
			<Button xmlns="sap.m" id="idCancelButtonDialog" press="closeDialog">
			</Button>
		</endButton>
	</Dialog>
</core:FragmentDefinition>