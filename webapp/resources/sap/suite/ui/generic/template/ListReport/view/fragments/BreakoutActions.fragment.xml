<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" 
	xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1" >

    <template:if test="{= ${parameter>/settings/quickVariantSelectionX}}">
        <template:then>
			<template:with path="tabItem>tableSettings/extensionActions" var="customActions">
				<core:Fragment fragmentName="sap.suite.ui.generic.template.ListReport.view.fragments.BreakoutActionButtons" type="XML"/>
			</template:with>
		</template:then>
        <template:else>
			<template:with path="parameter>/templateSpecific/tableSettings/extensionActions" var="customActions">
				<core:Fragment fragmentName="sap.suite.ui.generic.template.ListReport.view.fragments.BreakoutActionButtons" type="XML"/>
			</template:with>		
		</template:else>
	</template:if>
</core:FragmentDefinition>